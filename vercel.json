{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://orange-backend.vercel.app/"
      },
      {
        "src": "/auth/(.*)",
        "dest": "https://orange-backend.vercel.app/"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  